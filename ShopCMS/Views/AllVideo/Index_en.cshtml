        <div class="prs_video_section_main_wrapper" style="background:@("url("+Html.DisplayattachmentUrl(item.attachment.FileName,"LG")+") "))">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
