
@{

    Layout = "~/Views/Shared/_layout.cshtml";
}

@section Meta{
    @RenderSection("Meta", required: false)
}

@section Header{
    <link href="~/Content/Base/Scripts/sweetalert/sweetalert.css" rel="stylesheet" />
    <link href="~/Content/Default/user.css?v=1.3.5" rel="stylesheet" />

    @RenderSection("Header", required: false)
}

<div id="Profile" class="container">
    <div class="row">

        <div class="col-md-3" id="right-profile">
            <section>
                @{ Html.RenderAction("ProfileSideBar"); }
            </section>
        </div>
        <div class="col-md-9" id="left-profile">
            <section>
                @RenderBody()
            </section>
        </div>




    </div>

</div>


@section scripts{
    <script src="~/Content/Base/Scripts/sweetalert/sweetalert.min.js"></script>
    <script src="~/Content/Base/Scripts/view/Profile.js?v=1.2.1"></script>
    @RenderSection("scripts", required: false)
}