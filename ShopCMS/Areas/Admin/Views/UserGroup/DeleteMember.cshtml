@model Domain.UserGroupSelect
<title>حذف کاربر از گروه</title>


<div class="x_panel">
    <div class="x_content">
        <div class="text-left"><a class="btn btn-success" href="@Url.Action("addMember",new { id=Model.userGroupId})"> بازگشت به لیست <span class="glyphicon glyphicon-chevron-left"></span></a></div>

        <h1 style="display:inline-block;font-size: 2em;" class="box-title">
            <i class="flaticon-folder-filled-shape"></i> حذفِ کاربر از گروه
        </h1>
        <h4 style="color:red"><i class="glyphicon glyphicon-remove"></i> آیا مطمئن به حذف این رکورد هستید؟</h4>
        <br /><br />
        <p class="text text-danger"><strong>@ViewBag.Erorr</strong></p>
        <dl class="dl-horizontal">


        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="حذف" class="btn btn-danger" />
            </div>
        }
    </div>
</div>
